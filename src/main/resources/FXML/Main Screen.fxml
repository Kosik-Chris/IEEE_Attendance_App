<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controller.MainScreenController">
  <children>
    <MenuBar prefWidth="819.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Members">
               <items>
                  <MenuItem fx:id="newMemberItem" mnemonicParsing="false" onAction="#newMember" text="New Member" />
                  <MenuItem fx:id="editMemberItem" mnemonicParsing="false" onAction="#editMember" text="Edit Member" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Events">
               <items>
                  <MenuItem fx:id="newEventItem" mnemonicParsing="false" onAction="#newEvent" text="New Event" />
                  <MenuItem fx:id="editEventItem" mnemonicParsing="false" onAction="#editEvent" text="Edit Event" />
               </items></Menu>
        <Menu mnemonicParsing="false" text="Updates">
               <items>
                  <MenuItem fx:id="checkUpdatesItem" mnemonicParsing="false" onAction="#checkUpdates" text="Check for Updates" />
               </items></Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <ButtonBar layoutX="433.0" layoutY="335.0" prefHeight="40.0" prefWidth="200.0">
              <buttons>
                <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#logout" text="Logout" />
              </buttons>
            </ButtonBar>
      </children>
    </AnchorPane>
  </children>
</VBox>

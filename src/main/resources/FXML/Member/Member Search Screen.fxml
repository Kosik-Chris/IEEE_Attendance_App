<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.Controller.MemberSearchController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TitledPane animated="false" layoutY="-1.0" prefHeight="400.0" prefWidth="640.0" text="Member Search">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane layoutX="7.0" layoutY="2.0" prefHeight="372.0" prefWidth="386.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="366.0" prefWidth="376.0">
                                 <children>
                                    <ListView prefHeight="370.0" prefWidth="387.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                        <Pane layoutX="393.0" prefHeight="371.0" prefWidth="248.0">
                           <children>
                              <Button fx:id="homeBtn" layoutX="199.0" layoutY="2.0" mnemonicParsing="false" onAction="#returnHome" text="Home" />
                              <Text layoutX="14.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Member Name" />
                              <TextField fx:id="memberSearchField" layoutY="85.0" prefHeight="25.0" prefWidth="217.0" />
                              <Button fx:id="clearBtn" layoutX="157.0" layoutY="320.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="Clear" />
                              <Text layoutX="3.0" layoutY="148.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select members on left side then hit confirm" />
                              <Button fx:id="confirmBtn" layoutX="3.0" layoutY="320.0" mnemonicParsing="false" text="Confirm" />
                              <Text layoutX="3.0" layoutY="175.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select search to query Database" />
                              <Button fx:id="searchBtn" layoutX="83.0" layoutY="320.0" mnemonicParsing="false" text="Search" />
                           </children>
                        </Pane>
                     </children>
                  </AnchorPane>
              </content>
            </TitledPane>
         </children>
    </AnchorPane>
  </children>
</VBox>
